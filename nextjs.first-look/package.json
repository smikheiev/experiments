{
  "name": "nextjs",
  "private": true,
  "main": "./dist/nextServer.js",
  "scripts": {
    "dev": "next dev",
    "build:server": "tsc --project ./nextServer/tsconfig.json",
    "build:app": "next build",
    "build": "yarn build:server && yarn build:app",
    "serve": "yarn build && firebase emulators:start",
    "lint": "next lint",
    "deploy": "yarn build && firebase deploy --only functions,hosting"
  },
  "dependencies": {
    "date-fns": "2.28.0",
    "firebase-admin": "10.0.1",
    "firebase-functions": "3.16.0",
    "gray-matter": "4.0.3",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark": "14.0.2",
    "remark-html": "15.0.1"
  },
  "devDependencies": {
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.8",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": "16"
  }
}
